
# VITE_API_BASE_URL, VITE_AI_AGENTIC_URL, VITE_CODE_CONVERSION_URL ONLY ONES USED
# Frontend Application Environment Configuration
# Copy this file to .env for local development
# Never commit the actual .env file to version control

# =============================================================================
# APPLICATION ENVIRONMENT
# =============================================================================
NODE_ENV=development

# =============================================================================
# API CONFIGURATION
# =============================================================================
VITE_API_BASE_URL="https://hpcsiaipoc.azurewebsites.net"
VITE_API_TIMEOUT=30000

# =============================================================================
# WEBSOCKET CONFIGURATION
# =============================================================================
VITE_WS_URL=ws://localhost:8000/ws

# =============================================================================
# FEATURE FLAGS
# =============================================================================
VITE_ENABLE_CODE_CONVERSION=true
VITE_ENABLE_DATABASE_INSIGHTS=true
VITE_ENABLE_DOCUMENT_INSIGHTS=true
VITE_ENABLE_WEB_INSIGHTS=true
VITE_ENABLE_MEMBER_INSIGHTS=true
VITE_ENABLE_GRAPH_UI=true

# =============================================================================
# EXTERNAL SERVICES
# =============================================================================
# Code Conversion
VITE_CODE_CONVERSION_URL="https://hpcsiaipoc-codeconvert.azurewebsites.net"

# Database Services
VITE_DATABASE_INSIGHTS_URL=http://localhost:8000/api/database

VITE_DOC_INSIGHTS_URL="https://hpcsiaipoc-document-insights.azurewebsites.net"

# Document Processing
VITE_DOCUMENT_PROCESSING_URL=http://localhost:8000/api/documents

VITE_AI_AGENTIC_URL="https://hpcsiaipoc-agent-dev.azurewebsites.net"

# VITE_CODE_REVIEW_URL="https://hpcsiaipoc-codereview.azurewebsites.net"
VITE_CODE_REVIEW_URL="http://localhost:8004"

# =============================================================================
# DEBUG MODE
# =============================================================================
VITE_DEBUG=false

# =============================================================================
# NOTES
# =============================================================================
# - All Vite environment variables must start with VITE_
# - Restart the development server after changing environment variables
# - Use import.meta.env.VITE_VARIABLE_NAME to access in code
# - Boolean values should be strings: "true" or "false"
